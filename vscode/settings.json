{
  "editor.fontFamily": "'Hack', 'monospace', monospace",
  "editor.quickSuggestionsDelay": 5,
  "editor.suggest.preview": true,
  "editor.tabCompletion": "on",
  "editor.fontSize": 12,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": "inline"
  },
  "editor.cursorSurroundingLines": 15,
  "editor.minimap.enabled": false,
  "editor.cursorStyle": "block",
  "editor.renderWhitespace": "trailing",
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.showToolbar": "always",
  "editor.hover.delay": 150,
  "editor.largeFileOptimizations": false,
  "editor.wordBasedSuggestionsMode": "allDocuments",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    "[Catppuccin Macchiato]": {
      "comments": "#afb0b4",
      "textMateRules": [
        {
          "scope": [
            "string.quoted.docstring.multi.python",
            "source.python string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
            "source.python string.quoted.docstring.multi.python punctuation.definition.string.end.python",
            "source.python string.quoted.docstring.multi.python constant.character.escape.python"
          ],
          "settings": {
            "foreground": "#afb0b4"
          }
        }
      ]
    },
    "[Gruvbox Dark Hard]": {
      "comments": "#7e7e7e",
      "textMateRules": [
        {
          "scope": [
            "string.quoted.docstring.multi.python",
            "source.python string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
            "source.python string.quoted.docstring.multi.python punctuation.definition.string.end.python",
            "source.python string.quoted.docstring.multi.python constant.character.escape.python"
          ],
          "settings": {
            "foreground": "#7e7e7e"
          }
        }
      ]
    },
    "[Gruvbox Dark Medium]": {
      "comments": "#7e7e7e",
      "textMateRules": [
        {
          "scope": [
            "string.quoted.docstring.multi.python",
            "source.python string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
            "source.python string.quoted.docstring.multi.python punctuation.definition.string.end.python",
            "source.python string.quoted.docstring.multi.python constant.character.escape.python"
          ],
          "settings": {
            "foreground": "#7e7e7e"
          }
        }
      ]
    },
  },
  "workbench.startupEditor": "none",
  "workbench.editor.tabSizing": "shrink",
  "workbench.hover.delay": 300,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#b1ff63",
    "editorRuler.foreground": "#333333",
    "[Cold Horizon]": {
      "editorCursor.foreground": "#fafafa",
      "editor.selectionBackground": "#2fbce783",
      "editor.selectionHighlightBackground": "#2fbce783",
      // "editor.wordHighlightBackground": "#2fbce783",
      "editorBracketHighlight.foreground1": "#ffffff",
      "editorBracketHighlight.foreground2": "#1a7b99",
      "editorBracketHighlight.foreground3": "#00f2a1",
      "editorBracketHighlight.foreground4": "#d37239",
      "editorBracketHighlight.foreground5": "#ffffff",
      "editorBracketHighlight.foreground6": "#1a7b99"
    },
    "[Gruvbox Dark Hard]": {
      "editorCursor.foreground": "#B3AF25",
      "editorBracketHighlight.foreground1": "#B3AF25",
      "editorBracketHighlight.foreground2": "#8EB159",
      "editorBracketHighlight.foreground3": "#FE8019",
      "editorBracketHighlight.foreground4": "#ffffff",
      "editorBracketHighlight.foreground5": "#B3AF25",
      "editorBracketHighlight.foreground6": "#8EB159",
      "sideBar.border": "#1d2021",
      "statusBar.border": "#1d2021",
      "activityBar.border": "#1d2021",
      "editorGroup.border": "#1d2021",
      "editorGroupHeader.tabsBorder": "#1d2021",
      "editorLineNumber.foreground": "#464443"
    },
    "[Gruvbox Dark Medium]": {
      "editorCursor.foreground": "#B3AF25",
      "editorBracketHighlight.foreground1": "#B3AF25",
      "editorBracketHighlight.foreground2": "#8EB159",
      "editorBracketHighlight.foreground3": "#FE8019",
      "editorBracketHighlight.foreground4": "#ffffff",
      "editorBracketHighlight.foreground5": "#B3AF25",
      "editorBracketHighlight.foreground6": "#8EB159",
      "sideBar.border": "#282828",
      "statusBar.border": "#282828",
      "activityBar.border": "#282828",
      "editorGroup.border": "#282828",
      "editorGroupHeader.tabsBorder": "#282828",
      "editorLineNumber.foreground": "#464443"
    }
  },
  "security.workspace.trust.untrustedFiles": "open",
  "errorLens.delay": 300,
  "jupyter.interactiveWindow.creationMode": "perFile",
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    // "editor.rulers": [80, 100]
    "editor.rulers": [100]
  },
  // "pylint.args": [
  //   "--disable=C0103,C0114,C0115,C0116,C0413,E0401,E0611,E1101,E1111,W1203,E0601"
  // ],
  "pylint.args": [
    "--disable=C0103,C0114,C0116,C0413,E0401,E0611,E1101,W1203,E0601"
  ],
  "isort.args": ["--profile", "black"],
  "black-formatter.args": ["--line-length=100"],
  "flake8.args": ["--max-line-length=100"],
  "[javascript][typescript][yaml][javascriptreact][typescriptreact][html][css]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "trailing-spaces.logLevel": "info",
  "trailing-spaces.highlightCurrentLine": false,
  "trailing-spaces.borderColor": "rgba(255, 255, 255, 0.2)",
  "trailing-spaces.backgroundColor": "rgba(255, 255, 255, 0.2)",
  "debug.console.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "chat.editor.fontSize": 12,
  // "code-eol.newlineCharacter": "↓",
  "code-eol.newlineCharacter": "↵",
  "code-eol.returnCharacter": "←",
  "code-eol.crlfCharacter": "↵",
  "symbols.hidesExplorerArrows": false,
  "search.defaultViewMode": "tree",
  "search.showLineNumbers": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.silentNotifications": false,
  "zenMode.hideStatusBar": false,
  "zenMode.centerLayout": false,
  "trailing-spaces.trimOnSave": true,
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "githubPullRequests.pullBranch": "never",
  "workbench.tree.enableStickyScroll": false,
  "githubPullRequests.fileListLayout": "tree",
  "diffEditor.experimental.showMoves": true,
  "diffEditor.renderSideBySide": false,
  "window.customTitleBarVisibility": "auto",
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "workbench.layoutControl.enabled": false,
  "explorer.openEditors.visible": 5,
  "editor.stickyScroll.enabled": true,
  "editor.occurrencesHighlight": "multiFile",
  "gotoNextPreviousMember.symbolKinds": [
    "class",
    "function",
    "interface",
    "method",
    "module",
    "package"
  ],
  "glassit.alpha": 240,
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "workbench.activityBar.location": "hidden"
}
